    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "three": "^0.160.1",
    "tsx": "^3.12.5",
    "type-fest": "^3.6.1",
